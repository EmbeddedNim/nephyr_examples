/ {
  teensy4_header: connector {
    #gpio-cells = <2>;
    gpio-map-mask = <0xffffffff 0xfffffc00>;
    gpio-map-pass-thru = <0 0x3ff>;
    gpio-map =
        // /* GND */
         <0 0 &gpio1 3 0>,    /* RX1 */
         <1 0 &gpio1 2 0>,    /* TX1 */
         <2 0 &gpio4 4 0>,    /* D1 */
         <3 0 &gpio4 5 0>,    /* D2 */
         <4 0 &gpio4 6 0>,    /* D3 */
         <5 0 &gpio4 8 0>,    /* D4 */
         <6 0 &gpio2 10 0>,   /* D5 */
         <7 0 &gpio2 17 0>,   /* RX2 */
         <8 0 &gpio2 16 0>,   /* TX2 */
         <9 0 &gpio2 11 0>,   /* D6 */
         <10 0 &gpio2 0 0>,   /* CS0 */
         <11 0 &gpio2 2 0>,   /* MOSI */
         <12 0 &gpio2 1 0>,   /* MISO */
         // /* 3.3V       */
         // /* GND */
         <13 0 &gpio2 3 0>,    /* LED0 / SCK0 */
         <14 0 &gpio1 18 0>,   /* TX3 */
         <15 0 &gpio1 19 0>,   /* RX3 */
         <16 0 &gpio1 23 0>,   /* RX4 */
         <17 0 &gpio1 22 0>,   /* TX4 */
         <18 0 &gpio1 17 0>,   /* SDA */
         <19 0 &gpio1 16 0>,   /* SCL */
         <20 0 &gpio1 26 0>,   /* TX5 */
         <21 0 &gpio1 27 0>,   /* RX5 */
         <22 0 &gpio1 24 0>,   /* CTX1 */
         <23 0 &gpio1 25 0>;   /* CRX1 */
         // 

        //  <24 0 &gpio0 7 0>,   /* A10 / SCL2 / TX6 */
        //  <25 0 &gpio0 26 0>,  /* A11 / SDA2 / RX6 */
        //  <26 0 &gpio0 27 0>,  /* A12 / MOSI1 */
        //  <27 0 &gpio0 6 0>,   /* A13 / SCK1 */
        //  <28 0 &gpio0 8 0>,   /* RX7 */
        //  <29 0 &gpio1 9 0>,   /* TX7 */
        //  <30 0 &gpio0 8 0>,   /* CRX3 */
        //  <31 0 &gpio1 9 0>,   /* TX3 */
        //  <32 0 &gpio1 9 0>;   /* D7 */
  };
};

